{
    "languageserver": {
        "racket": {
            "command": "racket",
            "args": [
                "--lib",
                "racket-langserver"
            ],
            "filetypes": [
                "scheme"
            ]
        }
    },
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.snippetStatusText": "Ⓢ ",
    "coc.preferences.extensionUpdateCheck": "daily",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "suggest.completionItemKindLabels": {
        "class": "\uf0e8",
        "color": "\ue22b",
        "constant": "\uf8fe",
        "default": "\uf29c",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "event": "\ufacd",
        "field": "\uf93d",
        "file": "\uf723",
        "folder": "\uf115",
        "function": "\u0192",
        "interface": "\uf417",
        "keyword": "\uf1de",
        "method": "\uf6a6",
        "module": "\uf40d",
        "operator": "\uf915",
        "property": "\ue624",
        "reference": "\ufa46",
        "snippet": "\ue60b",
        "struct": "\ufb44",
        "text": "\ue612",
        "typeParameter": "\uf728",
        "unit": "\uf475",
        "value": "\uf89f",
        "variable": "\ue71b"
    },
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualTextPrefix": " ❯❯❯ ",
    "diagnostic.errorSign": "\uf467",
    "diagnostic.warningSign": "\uf071",
    "diagnostic.infoSign": "\uf129",
    "diagnostic.hintSign": "\uf864",
    "list.previewHighlightGroup": "Statement",
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.git.showIgnored": true,
    "explorer.buffer.showHiddenBuffers": false,
    "explorer.keyMappings.global": {
        "e": "nodePrev",
        "n": "nodeNext",
        "l": "toggleSelection",
        "<tab>": "actionMenu",
        "gl": "expandRecursive",
        "gh": "collapseRecursive",
        "i": "open",
        "<cr>": "open",
        "I": "openInVsplit",
        "o": "expand",
        "O": "openInTab",
        "u": "collapse",
        "h": "gotoParent",
        "yp": "copyFilepath",
        "yn": "copyFilename",
        "yy": "copyFile",
        "dd": "cutFile",
        "pp": "pasteFile",
        "dD": "deleteForever",
        "a": "addFile",
        "M": "addDirectory",
        "cw": "rename",
        ".": "toggleHidden",
        "zh": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "B": "gotoSource:file",
        "b": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
        "]c": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    },
    "leetcode.language":"cpp",
    "rust-analyzer.server.path":"/usr/bin/rust-analyzer",
    "clangd.semanticHighlighting":true,
    "clangd.fallbackFlags": ["-std=c++2a"]
}
